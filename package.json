  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/focusreactive/storybook-addon-graphcms.git"
  },
  "keywords": [
    "graphql",
    "graphcms",
    "query",
    "queries",
    "search",
    "storybookjs",
    "storybook-addon",
    "addon",
    "devtools"
